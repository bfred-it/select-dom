{
  "name": "select-dom",
  "version": "4.2.2",
  "description": "Extra lightweight DOM selector helper",
  "scripts": {
    "test": "tsc && tsd-check && xo && browserify -p esmify test.js | tape-run",
    "prepublish-only": "tsc",
    "build": "tsc",
    "watch": "tsc --watch"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "browserify": "^16.2.2",
    "eslint-config-xo-typescript": "^0.8.0",
    "esmify": "^2.1.1",
    "tape": "^4.9.0",
    "tape-run": "^4.0.0",
    "tsd-check": "^0.3.0",
    "typescript": "^3.3.3333",
    "xo": "*"
  },
  "keywords": [
    "dom",
    "elements",
    "css",
    "selector",
    "querySelector",
    "querySelectorAll",
    "selector",
    "jquery",
    "alternative"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "xo": {
    "extends": "xo-typescript",
    "extensions": [
      "ts"
    ],
    "envs": "browser",
    "rules": {
      "valid-jsdoc": 0,
      "prefer-arrow-callback": 0
    }
  },
  "license": "MIT",
  "repository": "bfred-it/select-dom",
  "author": "Federico Brigante <github@bfred.it> (bfred.it)"
}
